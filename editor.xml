<application>
  <component name="CodeInsightSettings">
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
    <EXCLUDED_PACKAGE NAME="org.junit.Assert.assertThrows" />
    <EXCLUDED_PACKAGE NAME="org.junit.Test" />
    <EXCLUDED_PACKAGE NAME="org.mockito.internal.verification.VerificationModeFactory.times" />
    <EXCLUDED_PACKAGE NAME="org.springframework.test.web.client.ExpectedCount.times" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="inheritors" />
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="PREVIEW" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="BIDI_TEXT_DIRECTION" value="LTR" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JAVA.java.implicit.types" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value=".problems" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bNotImplementedException\b" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
  </component>
</application>